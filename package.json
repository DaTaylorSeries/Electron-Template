{
  "name": "ElecctronTemplate",
  "email": "tjlarrechea@gmail.com",
  "url": "tjlarrechea.com",
  "version": "1.0.0",
  "description": "An Electron Templates",
  "main": "JS/Primary/main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "electrontemplate.app",
    "win": {
      "target": "zip"
    },
    "mac": {
      "category": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0"
  },
  "dependencies": {
    "electron-hot-reload": "^0.1.4",
    "electron-reload": "^1.5.0"
  }
}